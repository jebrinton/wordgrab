{
  "manifest_version": 3,
  "name": "Spanish Word Collector",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "48": "icons/ñ.png"
    }
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "icons": {
    "48": "icons/ñ.png" 
  }
}
